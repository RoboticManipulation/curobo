    <visual>
      <origin rpy="0 0 0" xyz="0.0 0 0"/>
      <geometry>
        <mesh filename="../../../grippers/schunk_wsg_50/meshes/WSG50_110.obj" scale="1 1 1"/>
      </geometry>
      <material name="Grey"/>
